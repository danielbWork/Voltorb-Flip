{
	"manifest_version": 2,
	"name": "Voltorb-Flip",
	"description": "Allows you to play voltorb flip",
	"version": "1.0.2",
	"icons": {
		"48": "assets/icons/voltorb_48x48.png",
		"96": "assets/icons/voltorb_96x96.png"
	},
	"permissions": ["storage","tabs"],
	"browser_action": {
		"default_icon": {
			"48": "assets/icons/voltorb_48x48.png",
			"96": "assets/icons/voltorb_96x96.png"
		},
		"default_popup": "src/browserAction/index.html",
		"default_title": "Voltorb-Flip"
	}
}